@misc{InfoT,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {Here we apply an information theory argument to obtain the distribution $P(x)$ of distances $x$ between nearest neighbor rods, when only the first moment $\langle x \rangle$ (which is the inverse of the rod density $\rho$) is known and thus no correlations are present. To obtain this distribution we will maximize the missing information $S$ given by: \begin{eqnarray*} S = - \int_0^{\infty} P(x) \, \ln P(x) \,dx \end{eqnarray*} under the constraint \begin{eqnarray*} \rho^{-1} = \langle x \rangle = \int_0^{\infty} x\, P(x) \,dx. \end{eqnarray*} Maximizing $S$ under this and the probability normalization constraint (using Lagrange multipliers) one finds that \cite{me:random-model} \begin{eqnarray*} P(x) = {1 \over \rho}\, e^{-{x \over \rho}}. \end{eqnarray*} We conclude that a uniform random distribution is generic for physical processes in which only the first moment (the density) is determined from outside. The above information-theory formalism can also be used to derive the distributions when higher moments are also given, which quantify correlations.}}

@misc{higherorder,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {We have also applied the ``Minkowski Sausage'' technique. In one dimension the two methods naturally provide identical results while we found that in two and three dimensions they slightly differ.\cite{me:random-model} Both methods measure $D_0$. The case of $D_q$, $q \neq 0$, will be treated elsewhere.}}

@misc{lowercutoff,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {By using boxes with size smaller than $r_0=d$, no additional information on the fine features of the object is obtained, and $D$ approaches the limit of $1$ as $r \rightarrow 0$.}}

@misc{sketch,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {The idea is to pick one box and then randomly deposit $M$ rods of size $d$ on the unit interval. The probability that the chosen box remains empty is $q = [ 1 - (r+d/L) ]^{M}$ and thus the probability that it will intersect at least one rod is $p=1-q$.}}

