@misc{Casimir-comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {A ``semi-simple'' Lie algebra does not possess an Abelian invariant subalgebra; e.g., $su(2)\otimes su(3)$. A ``simple'' Lie algebra is not Abelian and does not possess a proper invariant subalgebra; e.g., $su(N)$. If ${\cal L}$ is simple then it is also semi-simple, but not vice versa.\protect\cite{Cornwell:84II}}}

@misc{Casimir-comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {It can be shown that the following conditions must be satisfied:\cite{Alicki:87,Gantmacher:I} $\forall i$, $\mu_i,a_{ii} \geq 0$ where $\mu_i$ are the eigenvalues of $A$; $B_l \geq 0$, $1\leq l \leq M$, where $B_l$ is any leading submatrix of order $l$ obtained by deleting a number of rows, not necessarily of consecutive indices, and the corresponding columns.}}

@misc{Casimir-comment3,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {The 0-singlets are by definition the states that transform according to the 1-dimensional identity irrep of the group. Since $SU(N)$ group elements have the general form $g(\{\omega_\alpha\})=\exp(\sum_\alpha \omega_\alpha {\bf F}_\alpha)$, clearly Eq.(\ref{eq:singlet}) implies $g(\{\omega_\alpha\})|i_0\rangle = |i_0\rangle$, i.e., $|i_0\rangle$ transforms according to the identity irrep.}}

@misc{Casimir-comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {The most important of these conditions are the assumptions of memoriless (Markovian) dynamics, and ''complete positivity'': that when the S+B Hilbert space is extended by inclusion of {\it any} size system with zero internal and interaction Hamiltonian, $\rho$ still remains positive. It turns out that this requirement can only be satisified if initially system and bath are decoupled ($\rho \otimes \rho _{B}$), also known as the ''weak coupling limit''.\cite{Pechukas:94,Pechukas+Alicki:95}}}

@misc{DFD-comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {Other errors, e.g., a bit-flip which interchanges a logical 0 and 1, involve highly non-generic bath operations on the physical qubits, as can be cheed in the 4-qubit collective decoherence example, where the two singlet states are: $|{\tt 0}\ra \equiv |0011\ra -|0110\ra -|1001\ra +|1100\ra $ and $|{\tt 1} \ra \equiv |0101\ra -|0110\ra -|1001\ra +|1010\ra $ \cite{Zanardi:97c}. Such errors can thus safely be ignored.}}

@misc{DFD-comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {Since the irreps of a direct sum algebra are given by the direct product of the irreps of the constituent algebras \cite{Cornwell:84II}, the representation realized for the $\{\bF_{\alpha }\}$ is therefore in this case already irreducible and (since it is a tensor product of $K$ $2\times 2$ matrices) $2^{K}$ dimensional. Thus it cannot contain any 1D irreps.}}

@misc{EBB-comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {One way to see this is as follows. A density matrix $\rho $ in an $N$-dimensional Hilbert space can be described by an $(N^{2}-1)$-dimensional ``coherence vector'' $\overrightarrow{v}$ of real numbers (taking into account $\rho =\rho^{\dagger }$ and $\mathrm{Tr }\rho =1$), which is just the $N$-dimensional generalization of the Bloch vector \protect\cite{Alicki:87}. The most general transformation on this vector is a rotation $+$ contraction (described by an $(N^{2}-1) \times (N^{2}-1)$-dimensional real matrix $R$), followed by a translation (described by a vector $\overrightarrow{r}$ of dimension $N^{2}-1$): $\overrightarrow{v} \rightarrow R\overrightarrow{v}+\overrightarrow{r}$. This amounts to $ (N^{2}-1)\times (N^{2}-1)+N^{2}-1=N^{4}-N^{2}$ real parameters.}}

@misc{EBB-comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {Note that matrices in $SO(15)$ are $15^{2}=225$-dimensional; each rotation matrix $R^{ij;(k)}$ is indexed by $4$ indices $\alpha ,\beta ,\gamma ,\delta $, which range over $4$ values $\{0,x,y,z\}$; however, we must exclude from these $4^{4}$ possibilities the cases where $\alpha =\beta =0$ ($16$ cases, since $\gamma ,\delta $ are unrestricted), and vice versa, where $\gamma =\delta =0$ (another $16$ cases). Here we double-counted the case where $ \alpha =\beta =\gamma =\delta $, so in total we find $4^{4}-2\times 16+1=225$ allowed values of $\alpha ,\beta ,\gamma ,\delta $, in agreement with the dimension of $SO(15)$.}}

