@misc{random-comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {The Schulz distribution is a continuous version of the discrete Gamma distribution. Its standard formula is $P_S(a) = {1 \over \Gamma(m+1)} \left( {m+1 \over {\langle a \rangle}} \right)^{m+1} a^m\, e^{-(m+1)a/{\langle a \rangle}}$ (G. V. Schulz, Z. Phys. Chem. B {\bf 43}, 25 (1939)), where $m>-1$ determines the peakedness of the distribution. A general expression for the $n$th moment is: $\langle a^n \rangle = b^n \prod_{i=1}^n \left({a-b \over b}+i \right)$. The parameter $b$ we introduced is a measure of the width of the distribution: $b=\langle a \rangle-a^*$, where $a^*=\langle a \rangle {m \over m+1}$ is the position of the maximum of $P_S(a)$.}}

@misc{random-comment5,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {$\eta_n$ is an appropriate variable in the context of sequential addition of $n$-dimensional spheres, such as in an adsorption experiment. When one is a priori faced with the final result of overlapping spheres, such as in random spectra or the distribution of craters on an asteroid (M. J. S. Belton {\sl et al.}, Science, {\bf 265}, 1543 (1994)), it is more convenient to work with the {\em net} coverage, $\alpha^{(n)}(0) = 1-\exp \left(\eta_n \right)$, of Eq.(\ref{eq:M:alpha-ave}).}}

