@misc{fid:comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {For a quick derivation, suppose ${\bf A}$ diagonalizes $\brho^C(0)$: $[{\bf A} \brho^C(0) {\bf A}^{-1}]_{i j} \!=\! \lambda_i \delta_{i j}$. Since $\brho^C(0)$ is a density matrix, it is Hermitian so a (unitary) such ${\bf A}$ exists, and moreover $0 \le \lambda_i \le 1$, $\forall i$. Let $\brho'(t) \!\equiv\! {\bf A} \brho^C(t) {\bf A}^{-1}$. Then ${\rm Tr} \brho'(t) \!=\! {\rm Tr} \brho^C(t) \!=\! 1$ since the trace is invariant under cyclic permutation of its arguments and $\brho^C(t)$ is itself a density matrix. Now: ${\rm Tr} [\brho^C(0)\brho^C(t)] \!=\! {\rm Tr} [({\bf A} \brho^C(0) {\bf A}^{-1})\brho'(t)] \!=\! \sum_{i j} \lambda_i \delta_{i j} [\brho'(t)]_{j i} \!=\! \sum_i \lambda_i [\brho'(t)]_{i i} \le \sum_i [\brho'(t)]_{i i} \!=\! {\rm Tr} \brho'(t) \!=\! 1$, so $F(t) \!\le\! 1$. Equality is achieved only if just one of the $\lambda_i$'s is 1 (say $\lambda_1$, all others 0), i.e., is a pure state (then ${\bf A}$ is the identity), and at the same time $[\brho'(t)]_{1 1} \!=\! [\brho^C(t)]_{1 1} \!=\! 1$, i.e., $\brho^C(t) \!=\! \brho^C(0)$.}}

@misc{PRB:comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {It is also possible to relax the approximation (\ref{eq:sud1}) by using ${\bf g}'^2 \approx {\rm diag}\left( {\cal B}_n g_{\epsilon{\bf q}}^2 {\cal B}_n^{-1} \right)$ instead of $k_z^2 {\cal I}$. Here ${\cal B}$ is the matrix that diagonalizes the interaction matrix [see Eq.(\ref{eq:Q^D})] and ${\rm diag}(A)$ means the diagonal part of $A$.}}

@misc{PRB:comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {This non-truncation is a somewhat ambiguous requirement in the context of the SA: on the one hand it is in the spirit of the high-energy sudden since it corresponds to the existence of (infinitely) many open channels. On the other hand Eq.(\ref{eq:sud}) requires some channels to be closed.}}

