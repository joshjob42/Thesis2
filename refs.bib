	Keywords = {Condensed Matter - Mesoscale and Nanoscale Physics},
	Month = dec,
	Note = {arXiv:0807.4027 [cond-mat]},
	Number = {23},
	Title = {Kinetic {Equations} for {Transport} {Through} {Single}-{Molecule} {Transistors}},
	Url = {http://arxiv.org/abs/0807.4027},
	Urldate = {2014-01-23},
	Volume = {78},
