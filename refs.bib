	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:03 +0000},
	Note = {The reason for the square is that the power spectrum of the system-bath interaction amplitude is $J(\omega )=\gamma _{SB}^{2}\int_{-\infty }^{\infty }e^{-i\omega t}\langle B(t)B(0)\rangle $, (where $ B(t)=e^{iH_{B}t}Be^{-iH_{B}t}$ and angular brackets denote an average with respect to the bath state), and $T_{2}=2/J(0)$ \cite{Aihara:82}.}}

@misc{ERD:comment3,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:03 +0000},
	Note = {This corresponds to the case $\Delta \alpha =0$ in Eq.~(1) of \cite {Kielpinski:02}.}}

@misc{ERD:comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:03 +0000},
	Note = {The case $B_{\mathrm{oth}}^{z}\neq 0$ can also be the result of small departures from equal illumination during the SM\ logic gate \cite {Kielpinski:01}, as in the intensity and phase fluctuations of the exciting lasers analyzed in \cite{Schneider:98}. Hence, in order for the decoupling scheme to work the lasers used to implement the corresponding SM\ pulses must be more stable than the differential dephasing due to magnetic field fluctuation.}}

