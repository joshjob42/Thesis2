@misc{D-comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {It is also a hyperbolic distribution \cite{Mandelbrot}, in the sense that $P(l/L \geq \lambda) = 2(\lambda^{-D_c} - 1)/Z \sim \lambda^{-D_c}$. This follows from $P(l/L \geq 3^{-n}) = \sum_{i=0}^{n-1} P(l = 3^{i-n}) = \sum_{i=0}^{n-1} 2^{n-i}/Z = 2(2^n-1)/Z$ and $\lambda = 3^{-n}$.}}

@misc{D-comment5,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {Similar arguments were used recently [C. Tsallis, S.V.F. Levy, A.M.C. Souza and R. Maynard, Phys. Rev. Lett. {\bf 75}, 3589 (1995)] to argue for the ubiquity of L\'{e}vy distributions in Nature. In their formalism it is the {\em second} moment which is imposed as a constraint in an information theoretic argument.}}

