	Journal = {The European Physical Journal Special Topics},
	Number = {1},
	Pages = {75--88},
	Title = {Quantum annealing: The fastest route to quantum computation?},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1140/epjst/e2015-02344-2},
	Volume = {224},
