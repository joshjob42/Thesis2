@article{Hangleiter:2016uq,
	Author = {Hangleiter, D. and Kliesch, M. and Schwarz, M. and Eisert, J.},
	Date-Added = {2016-04-12 00:00:49 +0000},
	Date-Modified = {2016-04-12 00:00:49 +0000},
	Journal = {arXiv:1602.00703},
	N2 = {One of the main challenges in the field of quantum simulation and computation is to identify ways to certify the correct functioning of a device when a classical efficient simulation is not available. In such a situation one cannot simply classically keep track of the dynamics of the device. In this note, we discuss weak-membership notions of the certification for ground state preparations of frustration-free Hamiltonians based on simple energy measurements of local Hamiltonian terms. These considerations have applications to classically intractable analog quantum simulations: for example, using Feynman-Kitaev Hamiltonians, one can realise universal quantum computers. Our certification approach is non-interactive and thus avoids challenges arising from interactive proof systems. Moreover, our certification protocol is applicable to recent results on so-called IQP circuits aiming at the demonstration of quantum supremacy.},
	Title = {Direct certification of a class of quantum simulations},
	Ty = {JOUR},
	Url = {http://arXiv.org/abs/1602.00703},
	Year = {2016},
	Bdsk-Url-1 = {http://arXiv.org/abs/1602.00703%20},
	Bdsk-Url-2 = {http://arXiv.org/abs/1602.00703}}

