@misc{frac-scatt:comment0,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {It should be noted that these are {\em not} models of self-affine, rough surfaces, also considered in the literature \cite{Wong:2}.}}

@misc{frac-scatt:comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {A similar result was derived by Allain and Cloitre \cite{Allain:2,Allain:1}, in the case of optical scattering in the eikonal approximation, for doubly-infinite BU fractals.}}

@misc{frac-scatt:comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {Averaging the structure factor over the bands has been proposed by several authors \cite{Dettman,Makarov,Allain:2,Allain:1} to derive the power-law for iteratively constructed fractals. However, it is not clear how this procedure relates to experiments, in which the power law is observed without the need for any such averaging.}}

@misc{frac-scatt:comment3,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {It is tempting to consider also the polydisperse case, where dilations are chosen at random from a given probability distribution $P(s)$ (which is far more difficult). In this case one deals with a fractal operator of the type ${\cal F}^{\dag}_n = \prod_{j=1}^n (\openone + {\cal T}_{-a}\, {\cal C}_{1/s_j})$, for which it is still possible to obtain an explicit expression for the scattering intensities, using the ideas of the preceding calculations. This will, however, not be pursued any further here, since for random dilations the fractal dimension can only be defined {\em on average}, and thus ceases to be a useful characteristic of the scattering set: From Eq.(\ref{eq:D-here}), $\langle D \rangle = \log(2) \int ds\: P(s) 1/\log(1/s)$.}}

@misc{frac-scatt:comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {An example of a non-local potential is the so-called separable, or factorable potential $V=|\zeta\rangle \langle\zeta |$, where $\zeta$ is some fixed vector in the one-particle Hilbert space. Non-locality appears through $\langle \vec{x}'|V|\vec{x} \rangle = \zeta(\vec{x}')\zeta(\vec{x}^{*})$, unlike $\delta(\vec{x}'-\vec{x})$ for the matrix element of local potentials.}}

@misc{frac-scatt:comment5,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:01 +0000},
	Note = {A much richer class of dilations is possible by employing a general linear transformation, i.e., representing $s$ by matrix. Not all such transformations can be conveniently represented in exponential form with a single exponential, but many of them can. In particular, $\exp(x_i S_{ij} \partial_{x_j}) f(x) = f(s x), s=\exp(S)$. This general $s$ is capable of providing a simultaneous rotation and stretch along arbitrarily directed axes with individual factors for each axis.}}

