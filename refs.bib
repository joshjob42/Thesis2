@misc{SNUDD-comment,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:04 +0000},
	Note = {When dealing with concatenated codes, we will specialize our notation: our code will be a base $[[m,R,d_0]]$ code, with $\bar Q_0$ stabilizer generators, $K$ times concatenated, which will have resulting distance $d_k =d_0^k$ over $M=m^K$ qubits. We denote by $\{S_\mu^{[k]}\}$ the stabilizer generators of level $k$, $k=1,...,K$ and $\mu=1,...,\bar Q_0$, each of them acting on $n^k$ qubits. In this way the total number of stabilizer generators is $\sum_{k=1}^K \bar Q m^{k-1} $.}}

@misc{norms,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:04 +0000},
	Note = {For any linear operator $A$ with singular values $s_{i}(A)$ (eigenvalues of $\sqrt{A^{\dagger }A}$), $\Vert A\Vert \equiv \sup_{i}s_{i}(A)$ and $\Vert A\Vert _{1}\equiv \mathrm{Tr}\sqrt{A^{\dagger }A}=\sum_{i}s_{i}(A)$ are the standard sup-operator norm and trace norm, respectively.}}

