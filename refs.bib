@misc{com-nonunique,
	Date-Added = {2013-07-10 21:01:09 +0000},
	Date-Modified = {2013-07-10 21:01:10 +0000},
	Note = {{W}e stress that we are not restricted to the choice $V_{\alpha\beta}=\ln(p_\alpha/q_\beta)$. For example, if we choose $V_{\alpha\beta} = \ln \left( p_{\beta | \alpha} / f_\beta \right)$ then Eq.~\eqref{eq:3a} yields $P_{\mE}(v) e^{-v}= \sum_{\alpha,\beta} \delta(v - V_{\alpha\beta}) p_\alpha f_\beta$. In this case Eq.~\eqref{eqt:albash} immediately yields the QJE with $\gamma=1$.}}

@misc{2ndlaw-com,
	Date-Added = {2013-07-10 21:01:09 +0000},
	Date-Modified = {2013-07-10 21:01:10 +0000},
	Note = {{W}e further remark concerning Eq.~\eqref{II} that, upon applying $|\Tr[AB]| \leq \|A\|_\infty \|B\|_1$ (the product of the sup-operator and trace norms \cite{PhysRevA.78.012308}) to Eq.~\eqref{eq:gamma}, we find that $\gamma \le \|{\mE}(\ident)\|_\infty \le d$.}}

