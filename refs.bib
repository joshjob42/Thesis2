@article{delgado,
	Abstract = {We introduce the Uhlmann geometric phase as a tool to characterize symmetry protected topological phases in 1D fermion systems, such as topological insulators and superconductors. Since this phase is formulated for general mixed quantum states, it provides a way to to extend the notion of topological properties to finite temperature situations. We illustrate these ideas with some paradigmatic models and find that there exists a critical temperature $T_c$ at which the Uhlmann phase goes discontinuously and abruptly to zero. This stands a borderline between two different topological phases as a function of the temperature. Furthermore, at small temperatures we recover the usual notion of topological phase in fermion systems. },
	Author = {Viyuela, O. and Rivas, A. and Martin-Delgado, M. A.},
