	Journal = {Cybernetics, IEEE Transactions on},
	Journal1 = {Cybernetics, IEEE Transactions on},
	Keywords = {learning (artificial intelligence); minimisation; pattern classification; risk management; vectors; classification accuracy; classifiers; feature vector; impressive noise-tolerance properties; learning method; loss function; noise-free data; noise-tolerance properties; noise-tolerant learning algorithm; noisy data; risk minimization; squared error; unobservable training set; Fasteners; Noise; Noise measurement; Risk management; Training; Training data; Vectors; Label noise; loss functions; noise tolerance; risk minimization; Algorithms; Artificial Intelligence; Computer Simulation; Models, Statistical; Pattern Recognition, Automated; Risk Reduction Behavior; Signal-To-Noise Ratio},
	Number = {3},
