	Date1 = {2012-08-13 00:00:00},
	Journal = {Philosophical Transactions of the Royal Society of London A: Mathematical, Physical and Engineering Sciences},
	Number = {1972},
	Pages = {3750-3770},
	Title = {Exciton diffusion length in complex quantum systems: the effects of disorder and environmental fluctuations on symmetry-enhanced supertransfer},
	Type = {10.1098/rsta.2011.0213},
	Url = {http://rsta.royalsocietypublishing.org/roypta/370/1972/3750.full.pdf},
	Volume = {370},
