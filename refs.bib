@article{Chen:2016aa,
	Abstract = {In this paper, we propose an approach to interpret the prediction process of the BP-sLDA model, which is a supervised Latent Dirichlet Allocation model trained by Back Propagation over a deep architecture. The model is shown to achieve state-of-the-art prediction performance on several large-scale text analysis tasks. To interpret the prediction process of the model, often demanded by business data analytics applications, we perform evidence analysis on each pair-wise decision boundary over the topic distribution space, which is decomposed into a positive and a negative components. Then, for each element in the current document, a novel evidence score is defined by exploiting this topic decomposition and the generative nature of LDA. Then the score is used to rank the relative evidence of each element for the effectiveness of model prediction. We demonstrate the effectiveness of the method on a large-scale binary classification task on a corporate proprietary dataset with business-centric applications.},
	Author = {J. Chen and J. He and X. He and L. Xiao and J. Gao and L. Deng},
	Booktitle = {2016 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)},
	Date-Added = {2016-12-21 21:55:46 +0000},
	Date-Modified = {2016-12-21 21:57:26 +0000},
	Journal = {2016 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)},
	Journal1 = {2016 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)},
	Keywords = {backpropagation; business data processing; pattern classification; text analysis; BP-LDA model; LDA; back propagation; business data analytics; business-centric applications; deep architecture; evidence analysis; evidence score; large-scale binary classification task; large-scale text analysis tasks; pair-wise decision boundary; prediction process; supervised latent Dirichlet allocation model; supervised topic models; topic distribution space; Analytical models; Computer architecture; Logistics; Mirrors; Neural networks; Predictive models; Probabilistic logic; BP-sLDA; Topic model; back propagation; deep architecture; mirror descent},
	Pages = {2429--2433},
	Title = {Interpreting the prediction process of a deep network constructed from supervised topic models},
	Ty = {CONF},
	Url = {http://ieeexplore.ieee.org/document/7472113/},
	Year = {2016},
	Year1 = {20-25 March 2016},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/ICASSP.2016.7472113}}

