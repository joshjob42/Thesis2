@misc{QC:comment1,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {The ``$+$'' superscript indicates the ferromagnetic case.}}

@misc{QC:comment2,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {$S^{J_i}$ should be understood as ``S-plus-J'' or ``S-minus-J'' -- not ``$S$ to the power $J_i$''.}}

@misc{QC:comment3,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {Although one should keep in mind that the number of bits required for a desired accuracy of the computation must remain $O[\log(N)]$ \cite{Shor:94,Bennett:2}.}}

@misc{QC:comment4,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {This operator is still ``valid'', since it can be decomposed into a product of two-qubit operators.}}

@misc{QC:comment5,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {Technically this is an induction on a finite set, since the lattice is finite. But this is not a problem, since once the entire lattice is covered the operation of closing a new bond is undefined.}}

@misc{QC:comment6,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {This assumes that there is no reason to couple the temperature $T$ to $N$; such a situation will arise in the determination of the spin glass ground state, Sec.\ref{GS}.}}

@misc{QC:comment7,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {We refrain from explicitly writing out the Hamiltonians and algorithms in this section, since they are obvious extensions of the corresponding 2D expressions, and are unnecessarily cumbersome in notation.}}

@misc{QC:comment8,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {This is the minimal excitation of the $\pm J$ spin glass on the cubic lattice in any dimension: in 2D it corresponds to flipping a single spin with 3 satisfied and 1 unsatisfied neighbor; in 3D: 4 and 2, etc. Flipping a spin with equal numbers of satisfied and unsatisfied neighbors does not change the energy, and other combinations only increase the energy by more than $4J$.}}

@misc{QC:comment9,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {This simplification may be problematic in models of continuously distributed $J$'s with a vanishing gap, and will be addressed in a future publication. In any case, however, it is sufficient to find a polynomial-time algorithm for a {\em single} NP-complete problem, such as the $\pm J$ Ising spin glass in 3D.}}

@misc{QC:comment10,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {We cannot rule out the possibility that in 3D the increase of degeneracy with energy $E$, when integrated over $E$, will outweigh the Boltmann factor.}}

@misc{QC:comment11,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {In fact a quantum computer which is only allowed to use superposition, without interference, is equivalent in computational power to a classical probabilistic computer (D. Aharonov, private communication).}}

@misc{QC:comment12,
	Date-Added = {2013-07-10 20:58:01 +0000},
	Date-Modified = {2013-07-10 20:58:02 +0000},
	Note = {It should be stressed that in order to address the question of NP-completeness an algorithm should be able to deal with the {\em worst-case}. The present algorithm can most probably not do this since it is restricted in the class of bond-realizations it can simulate.}}

